<%= form_tag preview_path, method: :post, class: "form_tag" do %>
  <%= collection_select(:field, :name, Field.all, :name, :name, prompt: true) %>
  <input type="text" class="form_secondary" name="years" placeholder="Years of experience">
  <%= submit_tag "Submit", data: { disable_with: "Please wait.." }, class: "btn" %>
<% end %>

