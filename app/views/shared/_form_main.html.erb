    <%= simple_form_for @information do |f| %>
      <%= f.error_notification %>
      <%= f.input :salary, label: false, placeholder: "Salary" %>
      <%= f.input :company, label: false, placeholder: "Company" %>
      <%= f.input :years_worked, label: false, placeholder: "Years worked" %>
      <%= f.input :city, label: false, placeholder: "City" %>
      <%= f.button :submit "SEE REPORT" %>
    <% end %>
